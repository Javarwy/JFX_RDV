<?xml version="1.0" encoding="UTF-8"?>



        <?import javafx.geometry.Insets?>
        <?import javafx.scene.control.Button?>
        <?import javafx.scene.control.Label?>
        <?import javafx.scene.control.TableView?>
        <?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appli.secretaire.ListeDossierController">
<padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
</padding>

<Label fx:id="welcomeText" text="Liste des dossiers d'inscription" />
<VBox alignment="CENTER" prefHeight="800.0" prefWidth="600.0">
    <children>
        <TableView fx:id="tableauDossier" onMouseClicked="#onBara" />
    </children>
</VBox>
    <HBox alignment="CENTER">
        <Button onAction="#retour" text="Retour" />
    </HBox>
</VBox>
