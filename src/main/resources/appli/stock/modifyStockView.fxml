<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" prefHeight="563.0" prefWidth="716.0" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appli.fourniture.FournitureController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>

  <Label fx:id="welcomeText" text="Gestion Des Stocks" textOverrun="WORD_ELLIPSIS" underline="true">
    <font>
      <Font name="System Bold" size="24.0" />
    </font>
      <opaqueInsets>
         <Insets />
      </opaqueInsets></Label>
  <Label fx:id="emailText" text="Modification d'un fourniture :">
  <font>
    <Font name="System Bold Italic" size="14.0" />
  </font></Label>
   <Label fx:id="emailText2" text="Nom : ">
      <font>
         <Font name="System Italic" size="14.0" />
      </font>
   </Label>
   <TextField/>
   <Label fx:id="emailText21" text="Description : ">
      <font>
         <Font name="System Italic" size="14.0" />
      </font>
   </Label>
   <TextField/>
   <Label fx:id="emailText22" text="Prix : ">
      <font>
         <Font name="System Italic" size="14.0" />
      </font>
   </Label>
   <TextField />
   <Label fx:id="emailText221" text="Fournisseur : ">
      <font>
         <Font name="System Italic" size="14.0" />
      </font>
   </Label>
   <TextField />
   <Button onAction="#deconnexion" text="Annuler" />
   <Button onAction="#ajoutFourniture" text="Confirmer Modification" />
</VBox>
